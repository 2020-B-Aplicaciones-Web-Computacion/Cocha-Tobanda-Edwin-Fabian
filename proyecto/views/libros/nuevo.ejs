<form action="/libro/crear-libro" method="post">
    <label for="isbn">ISBN</label>
    <input id="isbn" type="text" name="isbn">
    <br>
    <label for="titulo">Título</label>
    <input id="titulo" type="text" name="titulo">
    <br>
    <label for="anio">Año Publicación</label>
    <input id="anio" type="text" name="anio">
    <br>
    <label for="idioma">Idioma</label>
    <input id="idioma" type="text" name="idioma">
    <br>
    <label for="pais">País Publicación</label>
    <input id="pais" type="text" name="pais">
    <br>
    <label for="autor">Id Autor</label>
    <input id="autor" type="text" name="autor" placeholder="<%= libro.fkAutor %>">
    <br>
    <button type="submit">Crear</button>
    <button type="reset">Reset</button>
</form>
<a href="/libro/libros">
    <button>Cancelar</button>
</a>